#include<iostream>
using namespace std;
};
class Animal{
public:
virtual void makesound()=0;
class Dog: public Animal{
};
public:
void makesound() override { cout<<"Dog says woof woof...!!"<<"\n";
}
class Cat: public Animal{
};
public:
void makesound() override. { cout<<"Cat says meow meow...!!"<<endl;
};
int main(){
Dog pet1;
Cat pet2;
pet1.makesound();
pet2.makesound();
}
